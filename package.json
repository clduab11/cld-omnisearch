    "jina-ai",
    "llm",
    "content-processing",
    "summarization",
    "fact-verification",
    "content-enrichment",
    "url-reader",
    "pdf-processing",
    "image-captioning",
    "unified-search",
    "multi-provider"
  ],
  "author": "Scott Spence",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@types/node": "^22.13.8",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.1"
