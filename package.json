{
  "name": "mcp-omnisearch",
  "version": "0.0.1",
  "description": "MCP server for integrating Omnisearch with LLMs",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mcp-omnisearch": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
